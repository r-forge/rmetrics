citHeader("To cite package 'fCertificates' in publications use:")
desc <- packageDescription("fCertificates")
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", desc$Date)
vers <- paste("R package version", desc$Version)
citEntry(entry="Manual",
title = "fCertificates: Basics of Structured Products Valuation",
author = personList(as.person("Stefan Wilhelm")),
year = year,
note = vers,
textVersion =
paste("Stefan Wilhelm (",
year,
"). fCertificates: Basics of Structured Products Valuation. ",
vers, ".", sep=""))
