citEntry(entry="Manual",
         title = "rngWELL: WELL generators.",
         author = "Christophe Dutang, Petr Savicky and Diethelm Wuertz",
	 year = 2009,
         note = "R package version 1.00",

         textVersion =
	 paste(	"C. Dutang et al.",
		"(2009),",
	       	"rngWELL: WELL generators.")
)

