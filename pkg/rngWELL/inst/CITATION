citHeader("To cite rngWELL in publications use:")


year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "rngWELL: WELL generators",
         author = c(person("Dutang", "Christophe"), person("Savicky","Petr")),
         year = year,
         note = note)
